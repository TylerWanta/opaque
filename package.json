{
  "name": "opaque-monorepo",
  "version": "0.2.0",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7"
  },
  "scripts": {
    "build": "node ./bin/build.js",
    "example:client:dev": "cd example-client && pnpm dev",
    "example:server:dev": "cd example-server && pnpm dev",
    "test:p256": "cross-env OPAQUE_BUILD=p256 jest",
    "test:ristretto": "jest",
    "test": "pnpm test:p256 && pnpm test:ristretto",
    "publish:ristretto": "cd build/ristretto && pnpm publish",
    "publish:p256": "cd build/p256 && pnpm publish",
    "publish": "pnpm publish:ristretto && pnpm publish:p256"
  }
}
